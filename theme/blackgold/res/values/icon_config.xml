        <!-- VVETIME投影 -->
        <item>com.android.settings|com.android.settings$com.android.settings.Settings$HdmiSettingsActivity|VVETIME投影#ic_vvetimet.png</item>
        <!-- 通讯录 -->
        <item>com.android.contacts|com.android.contacts$com.android.contacts.activities.PeopleActivity|通讯录#ic_contacts.png</item>
