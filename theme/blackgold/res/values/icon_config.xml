<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<resources>
    <!--主题名字-->
    <string name="theme_name">ZY Theme</string>
    <!--主题版本号-->
    <string name="theme_version">v1.1</string>
    <!--主题类型是否为异型-->
    <bool name="is_heteromorphic_theme">false</bool>
    <!--三方没有重绘的图标在底板中的显示位置-->
    <dimen name="ptop">0dp</dimen>

    <!--日历日期文本颜色-->
    <color name="com.android.launcher3$dym_calendar_day_text">#eac796</color>
    <!--日历星期文本颜色-->
    <color name="com.android.launcher3$dym_calendar_week_text">#eac796</color>
    <!--天气文本颜色-->
    <color name="com.android.launcher3$dym_weather_text">#eac796</color>

    <string-array name="icon_array">
